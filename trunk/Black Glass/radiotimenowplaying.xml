<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
	<id>25652</id>
	<defaultcontrol>155</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<define>#header.label:Radio Time</define>
	<define>#usePlayFanart:Yes</define>
	<animation effect="fade" time="250">WindowOpen</animation>
	<animation effect="fade" time="250">WindowClose</animation>
	<controls>
		<import>common.window.xml</import>
		<import>common.music.play.fanart.xml</import>
		<import>common.time.xml</import>



		<control>
			<id>112</id>
			<description>album art image</description>
			<type>image</type>
			<posX>40</posX>
			<posY>90</posY>
			<width>342</width>
			<height>275</height>
			<texture flipY="true" diffuse="Thumb_Mask.png">#RadioTime.Play.Image</texture>
			<keepaspectratio>yes</keepaspectratio>
			<centered>yes</centered>
		</control>
		<control>
			<type>image</type>
			<id>0</id>
			<posX>30</posX>
			<posY>80</posY>
			<width>360</width>
			<height>293</height>
			<texture flipY="true" diffuse="Thumb_Mask.png">PictureCase.png</texture>
		</control>



		<control>
			<id>0</id>
			<description>Station label</description>
			<type>fadelabel</type>
			<posX>30</posX>
			<posY>390</posY>
			<width>600</width>
			<height>25</height>
			<font>font24</font>
			<label>#RadioTime.Play.Station</label>
			<wrapString> : : : </wrapString>
		</control>
		<control>
			<id>0</id>
			<description>Slogan label</description>
			<type>fadelabel</type>
			<posX>30</posX>
			<posY>430</posY>
			<width>600</width>
			<font>font14</font>
