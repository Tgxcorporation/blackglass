			<wrapString> : : : </wrapString>
		</control>

		<control>
			<id>0</id>
			<description>Location label</description>
			<type>fadelabel</type>
			<posX>30</posX>
			<posY>600</posY>
			<width>600</width>
			<font>font22</font>
			<textcolor>ffffffff</textcolor>
