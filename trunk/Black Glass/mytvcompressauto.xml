<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
	<id>611</id>
	<defaultcontrol>2</defaultcontrol>
	<allowoverlay>player.hasmedia+!facadeview.filmstrip</allowoverlay>
	<define>#header.label:895</define>
	<animation effect="fade" time="250" start="100" end="100" delay="250">WindowClose</animation>
	<controls>
		<import>common.window.xml</import>
		<control>
			<description>Date</description>
			<type>label</type>
			<id>200</id>
			<posX>1226</posX>
			<posY>37</posY>
			<label>#date</label>
			<align>right</align>
			<font>font13</font>
			<textcolor>ffa9d0f7</textcolor>
		</control>
		<control>
			<description>Time</description>
			<type>label</type>
			<id>201</id>
			<posX>1226</posX>
			<posY>62</posY>
			<label>#time</label>
			<align>right</align>
			<font>font13</font>
			<textcolor>ffa9d0f7</textcolor>
		</control>
		<control>
			<description>Enable auto compress</description>
			<type>checkmark</type>
			<id>2</id>
			<posX>512</posX>
			<posY>121</posY>
			<label>994</label>
			<textcolor>FFFFFFFF</textcolor>
			<onleft>2</onleft>
			<onright>2</onright>
			<onup>17</onup>
			<ondown>5</ondown>
		</control>
		<control>
			<description>Auto compress label</description>
			<type>label</type>
			<id>4</id>
			<posX>512</posX>
			<posY>168</posY>
			<label>895</label>
			<font>font13</font>
			<textcolor>FFFFFFFF</textcolor>
		</control>
		<control>
			<description>Auto compress select</description>
			<type>spincontrol</type>
			<id>5</id>
			<posX>977</posX>
			<posY>173</posY>
			<onup>2</onup>
			<ondown>7</ondown>
			<onleft>5</onleft>
			<onright>5</onright>
			<align>right</align>
			<textcolor>FFFFFFFF</textcolor>
			<showrange>no</showrange>
			<subitems>
				<subitem>1 Hour</subitem>
				<subitem>2 Hour</subitem>
				<subitem>3 Hour</subitem>
				<subitem>4 Hour</subitem>
				<subitem>5 Hour</subitem>
				<subitem>6 Hour</subitem>
				<subitem>7 Hour</subitem>
				<subitem>8 Hour</subitem>
			</subitems>
			<textureUp>arrow_round_right_nofocus.png</textureUp>
			<textureDown>arrow_round_left_nofocus.png</textureDown>
			<textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
			<textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
		</control>
		<control>
			<description>Delete original file</description>
			<type>checkmark</type>
			<id>7</id>
			<posX>512</posX>
			<posY>211</posY>
			<label>992</label>
			<textcolor>FFFFFFFF</textcolor>
			<onleft>15</onleft>
			<onright>15</onright>
			<onup>5</onup>
			<ondown>99</ondown>
		</control>
	</controls>
</window>