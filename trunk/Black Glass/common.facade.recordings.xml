<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
	<controls>
		<control>
			<id>0</id>
			<type>image</type>
			<posX>-200</posX>
			<posY>550</posY>
			<width>1680</width>
			<height>180</height>
			<texture>gradient.png</texture>
			<visible>facadeview.filmstrip</visible>
		</control>
		<control>
			<description>group element</description>
			<type>group</type>
			<animation effect="slide" tween="circle" easing="out" time="200" start="900,0" reversible="false">WindowOpen</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" end="900,0" reversible="false">WindowClose</animation>
			<control>
				<description>composite control consisting of a list control and a thumbnail panel</description>
				<type>facadeview</type>
				<id>50</id>
				<control>
					<description>listcontrol</description>
					<type>listcontrol</type>
					<id>50</id>
					<onleft>2</onleft>
					<onright>2</onright>
					<scrollOffset>2</scrollOffset>
				</control>
				<control>
					<description>album view listcontrol</description>
					<type>listcontrol</type>
					<subtype>album</subtype>
					<id>50</id>
					<textXOff2>770</textXOff2>
					<textYOff2>65</textYOff2>
					<textYOff3>65</textYOff3>
					<textXOff2>770</textXOff2>
					<textXOff3>140</textXOff3>
					<font>font20</font>
					<itemWidth>120</itemWidth>
					<itemHeight>90</itemHeight>
					<IconYOff>8</IconYOff>
					<textureHeight>107</textureHeight>
					<onleft>2</onleft>
					<onright>50</onright>
					<onup>2</onup>
					<ondown>2</ondown>
					<textureFocus>bg_facade2_line_focus.png</textureFocus>
					<textureNoFocus>bg_facade2_line_nofocus.png</textureNoFocus>
					<PinIconXOff>776</PinIconXOff> 
					<PinIconYOff>39</PinIconYOff>
				</control>
				<control>
					<description>Thumbnail Panel</description>
					<type>thumbnailpanel</type>
					<id>50</id>
					<onleft>2</onleft>
					<onright>2</onright>
					<posX>500</posX>
					<posY>110</posY>
					<width>815</width>
					<height>600</height>
					<scrollOffset>1</scrollOffset>
					<itemWidth>98</itemWidth>
					<itemHeight>87</itemHeight>
					<textureWidth>91</textureWidth>
					<textureHeight>60</textureHeight>
					<thumbWidth>85</thumbWidth>
					<thumbHeight>47</thumbHeight>
					<thumbPosX>3</thumbPosX>
					<thumbPosY>3</thumbPosY>
					<itemWidthBig>198</itemWidthBig>
					<itemHeightBig>135</itemHeightBig>
					<thumbWidthBig>156</thumbWidthBig>
					<thumbHeightBig>86</thumbHeightBig>
					<textureWidthBig>165</textureWidthBig>
					<textureHeightBig>107</textureHeightBig>
					<thumbPosXBig>4</thumbPosXBig>
					<thumbPosYBig>4</thumbPosYBig>
					<unfocusedAlpha>120</unfocusedAlpha>
					<hideUnfocusTexture>no</hideUnfocusTexture>
					<renderFocusText>yes</renderFocusText>
					<renderUnfocusText>yes</renderUnfocusText>
					<imageFolder>-</imageFolder>
					<imageFolderFocus>-</imageFolderFocus>
					<thumbZoom>no</thumbZoom>
					<enableFocusZoom>no</enableFocusZoom>
					<textXOff>1</textXOff>
					<textYOff>2</textYOff>
					<spinCanFocus>no</spinCanFocus>
					<showFrame>true</showFrame>
					<frameNoFocus>VideoCase.png</frameNoFocus> 
					<frameFocus>VideoCase.png</frameFocus> 
				</control>
				<control>
					<description>Filmstrip view</description>
					<type>filmstrip</type>
					<dimColor>90ffffff</dimColor>
					<id>50</id>
					<scrollOffset>2</scrollOffset>
					<onup>2</onup>
					<onleft>2</onleft>
					<posX>20</posX>
					<posY>470</posY>
					<width>1350</width>
					<thumbWidth>227</thumbWidth>
					<thumbHeight>127</thumbHeight>
					<thumbPosX>7</thumbPosX>
					<thumbPosY>6</thumbPosY>
					<itemWidth>253</itemWidth>
					<itemHeight>280</itemHeight>
					<textureWidth>240</textureWidth>
					<textureHeight>158</textureHeight>
					<showFrame>no</showFrame>
					<frame>-</frame>
					<frameFocus>-</frameFocus>
					<showFolder>yes</showFolder>
					<imageFolder flipY="true" diffuse="Thumb_Mask.png">VideoCase.png</imageFolder>
					<imageFolderFocus>VideoCase.png</imageFolderFocus>
					<thumbs flipY="true" diffuse="Thumb_Mask.png"></thumbs>
					<showBackGround>no</showBackGround>
					<background>-</background>
					<showInfoImage>no</showInfoImage>
					<InfoImage>-</InfoImage>
					<visible>facadeview.filmstrip</visible>
					<thumbAnimation effect="slide" end="0,-15" time="200">focus</thumbAnimation>
					<thumbAnimation effect="zoom" start="100,100" end="120,120" time="200">focus</thumbAnimation>
					<scrollOffset>3</scrollOffset>
				</control>
				<control>
					<description>playlist listcontrol</description>
					<type>playlistcontrol</type>
					<id>50</id>
					<onleft>2</onleft>
					<onright>2</onright>
					<itemWidth>40</itemWidth>
					<itemHeight>30</itemHeight>
					<IconXOff>10</IconXOff>
					<IconYOff>11</IconYOff>
					<scrollOffset>2</scrollOffset>
				</control>
			</control>
		</control>
	</controls>
</window>