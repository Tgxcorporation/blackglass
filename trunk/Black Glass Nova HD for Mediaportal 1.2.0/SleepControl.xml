<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>7969</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>player.hasmedia</allowoverlay>
  <define>#header.label:Sleep Control</define>
  <define>#header.background:hover_sleepcontrol.png</define>
  <animation effect="fade" time="250">WindowOpen</animation>
  <animation effect="fade" time="250">WindowClose</animation>
  <controls>
    <import>common.window.xml</import>
    <import>common.window.time.xml</import>
    <control>
      <description>Status</description>
      <type>fadelabel</type>
      <id>0</id>
      <posX>45</posX>
      <posY>84</posY>
      <font>font11</font>
      <width>1125</width>
      <label>#SleepControl.Text</label>
      <wrapString> : : : </wrapString>
    </control>
    <!-- *** Sleep Activity *** -->
    <control>
      <description>Sleep Activity</description>
      <type>label</type>
      <id>0</id>
      <posX>360</posX>
      <posY>397</posY>
      <width>1200</width>
      <label>#SleepControl.Activity</label>
      <align>center</align>
      <font>font37</font>
      <textcolor>ffffffff</textcolor>
    </control>
    <!-- *** Sleep Method *** -->
    <control>
      <description>Sleep Method</description>
      <type>label</type>
      <id>0</id>
      <posX>360</posX>
      <posY>472</posY>
      <width>1200</width>
      <label>#SleepControl.Method</label>
      <align>center</align>
      <font>font16</font>
      <textcolor>90ffffff</textcolor>
    </control>
    <!-- *** Sleep Counter *** -->
    <control>
      <description>Sleep Counter</description>
      <type>label</type>
      <id>0</id>
      <posX>360</posX>
      <posY>577</posY>
      <width>1200</width>
      <label>#SleepControl.Counter</label>
      <align>center</align>
      <font>font37</font>
      <textcolor>ffffffff</textcolor>
    </control>
    <!-- *** Sleep Start/End *** -->
    <control>
      <description>Sleep Method</description>
      <type>label</type>
      <id>0</id>
      <posX>360</posX>
      <posY>652</posY>
      <width>1200</width>
      <label>#SleepControl.Start #SleepControl.End</label>
      <align>center</align>
      <font>font16</font>
      <textcolor>90ffffff</textcolor>
    </control>
    <!-- *** Sleep Status *** -->
    <control>
      <description>Sleep Status</description>
      <type>label</type>
      <id>0</id>
      <posX>360</posX>
      <posY>712</posY>
      <width>1200</width>
      <label>#SleepControl.Status</label>
      <align>center</align>
      <font>font16</font>
      <textcolor>90ffffff</textcolor>
    </control>
    <!-- *** Button *** -->
    <control>
      <type>image</type>
      <id>0</id>
      <posX>-579</posX>
      <posY>4</posY>
      <width>627</width>
      <height>1071</height>
      <texture>bg_left.png</texture>
      <colorDiffuse>ffffffff</colorDiffuse>
      <visible>![control.hasfocus(2)|control.hasfocus(3)|control.hasfocus(4)|control.hasfocus(5)|control.hasfocus(6)|control.hasfocus(7)|control.hasfocus(8)|control.hasfocus(9)]</visible>
      <animation effect="fade" time="50">WindowClose</animation>
      <animation effect="fade" time="50">WindowOpen</animation>
      <animation effect="fade" time="250">visiblechange</animation>
    </control>
    <control>
      <type>actiongroup</type>
      <description>action menu</description>
      <defaultcontrol>2</defaultcontrol>
      <onexit>0</onexit>
      <buttonX>0</buttonX>
      <buttonY>0</buttonY>
      <buttonwidth>84</buttonwidth>
      <buttonheight>1080</buttonheight>
      <textureFocus>-</textureFocus>
      <textureNoFocus>-</textureNoFocus>
      <width>637</width>
      <height>1080</height>
      <dimColor>ffffffff</dimColor>
      <visible>control.hasfocus(2)|control.hasfocus(3)|control.hasfocus(4)|control.hasfocus(5)|control.hasfocus(6)|control.hasfocus(7)|control.hasfocus(8)|control.hasfocus(9)</visible>
      <animation effect="fade" time="250" start="75" reversible="false">visible</animation>
      <animation effect="slide" tween="circle" easing="out" time="250" start="-312,0" reversible="false">visible</animation>
      <animation effect="fade" time="250" end="75" reversible="false">hidden</animation>
      <animation effect="slide" tween="circle" easing="out" time="250" end="-312,0" reversible="false">hidden</animation>
      <animation effect="slide" time="150" end="-400,0" reversible="false">WindowClose</animation>
      <animation effect="slide" tween="circle" easing="out" time="250" start="-312,0" reversible="false">WindowOpen</animation>
      <animation effect="fade" time="250" start="75" reversible="false">WindowOpen</animation>
      <control>
        <type>image</type>
        <id>0</id>
        <posX>0</posX>
        <posY>0</posY>
        <width>627</width>
        <height>1080</height>
        <texture>bg_left.png</texture>
        <colorDiffuse>ffffffff</colorDiffuse>
      </control>
      <control>
        <id>0</id>
        <type>label</type>
        <posX>45</posX>
        <posY>52</posY>
        <label>#header.label</label><width>500</width>
        <font>font24</font>
        <align>left</align>
        <textcolor>ffffffff</textcolor>
      </control>
      <control>
        <type>image</type>
        <id>0</id>
        <posX>0</posX>
        <posY>71</posY>
        <width>579</width>
        <height>79</height>
        <texture>bg_button_nofocus.png</texture>
      </control>
      <control>
        <type>group</type>
        <layout>StackLayout(0, Vertical, true)</layout>
        <posX>0</posX>
        <posY>150</posY>
        <control>
          <description>Sleep Mode</description>
          <type>button</type>
          <id>2</id>
          <label>Sleep Mode</label>
          <onup>17</onup>
          <ondown>3</ondown>
          <onright>50</onright>
        </control>
        <control>
          <description>Set Timer</description>
          <type>button</type>
          <id>3</id>
          <label>Set Timer</label>
          <onup>2</onup>
          <ondown>4</ondown>
          <onright>50</onright>
        </control>
        <control>
          <description>Start Timer</description>
          <type>button</type>
          <id>4</id>
          <label>Start Timer</label>
          <onup>3</onup>
          <ondown>5</ondown>
          <onright>50</onright>
        </control>
        <control>
          <description>Pause timer</description>
          <type>button</type>
          <id>5</id>
          <label>Pause timer</label>
          <onup>4</onup>
          <ondown>6</ondown>
          <onright>50</onright>
        </control>
        <control>
          <description>Increment Timer</description>
          <type>button</type>
          <id>6</id>
          <label>Increment Timer</label>
          <onup>5</onup>
          <ondown>7</ondown>
          <onright>50</onright>
        </control>
        <control>
          <description>Decrement Timer</description>
          <type>button</type>
          <id>7</id>
          <label>Decrement Timer</label>
          <onup>6</onup>
          <ondown>8</ondown>
          <onright>50</onright>
        </control>
        <control>
          <description>Wait until tasks finished</description>
          <type>togglebutton</type>
          <id>8</id>
          <label>Wait until tasks finished</label>
          <onup>7</onup>
          <ondown>9</ondown>
          <onright>50</onright>
        </control>
        <control>
          <description>Volume Reduction</description>
          <type>togglebutton</type>
          <id>9</id>
          <label>Volume Reduction</label>
          <onup>8</onup>
          <ondown>2</ondown>
          <onright>50</onright>
        </control>
      </control>
    </control>
    <control>
      <description>Dummy Button</description>
      <type>button</type>
      <id>50</id>
      <label>-</label>
      <onup>2</onup>
      <ondown>2</ondown>
      <onleft>2</onleft>
      <onright>2</onright>
      <textureFocus>-</textureFocus>
      <textureNoFocus>-</textureNoFocus>
    </control>
  </controls>
</window>