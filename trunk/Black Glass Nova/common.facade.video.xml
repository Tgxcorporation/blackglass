<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>

	<controls>
		<control>
			<description>group element</description>
			<type>group</type>
			<control>
				<description>composite control consisting of a list control and a thumbnail panel</description>
				<type>facadeview</type>
				<id>50</id>

				<control>
					<description>listcontrol</description>
					<type>listcontrol</type>
					<id>50</id>
					<onleft>2</onleft>
					<onright>2</onright>
					<scrollOffset>2</scrollOffset>
					<posX>640</posX>
					<width>640</width>
					<posY>430</posY>
					<height>300</height>
					<textureHeight>35</textureHeight>
					<itemWidth>40</itemWidth>
					<itemHeight>30</itemHeight>
					<IconXOff>10</IconXOff>
					<IconYOff>2</IconYOff>
					<textXOff2>630</textXOff2>
					<textYOff2>2</textYOff2>
					<textXOff3>630</textXOff3>
					<textYOff3>2</textYOff3>
					<font>font14</font>	
					<font2>font14</font2>	
					<font3>font14</font3>	
					<textYOff>2</textYOff>
					<spinWidth>30</spinWidth>
					<spinHeight>30</spinHeight>
					<spinPosX>1210</spinPosX>
					<spinPosY>685</spinPosY>
					<PinIconXOff>627</PinIconXOff> 
					<PinIconYOff>4</PinIconYOff>

				</control>
				<control>
					<description>Thumbnail Panel</description>
					<type>thumbnailpanel</type>
					<id>50</id>
					<onleft>2</onleft>
					<onright>2</onright>
					<posX>25</posX>
					<posY>250</posY>
					<width>1340</width>
					<height>480</height>
					<scrollOffset>1</scrollOffset>
					<itemWidth>90</itemWidth>
					<itemHeight>126</itemHeight>
					<textureWidth>77</textureWidth>
					<textureHeight>98</textureHeight>
					<thumbWidth>66</thumbWidth>
					<thumbHeight>94</thumbHeight>
					<thumbPosX>9</thumbPosX>
					<thumbPosY>2</thumbPosY>
					<itemWidthBig>140</itemWidthBig>
					<itemHeightBig>190</itemHeightBig>
					<thumbWidthBig>106</thumbWidthBig>
					<thumbHeightBig>151</thumbHeightBig>
					<textureWidthBig>123</textureWidthBig>
					<textureHeightBig>157</textureHeightBig>
					<thumbPosXBig>14</thumbPosXBig>
					<thumbPosYBig>3</thumbPosYBig>
					<thumbZoom>no</thumbZoom>
					<enableFocusZoom>no</enableFocusZoom>
					<hideUnfocusTexture>no</hideUnfocusTexture>
					<renderFocusText>yes</renderFocusText>
					<renderUnfocusText>yes</renderUnfocusText>
					<imageFolder>DVDCase.png</imageFolder>
					<imageFolderFocus>DVDCase.png</imageFolderFocus>
					<textXOff>2</textXOff>
					<textYOff>2</textYOff>
					<spinCanFocus>no</spinCanFocus>
					<unfocusedAlpha>120</unfocusedAlpha>
					<showFrame>true</showFrame>
					<frameNoFocus>DVDCase.png</frameNoFocus> 
					<frameFocus>DVDCase.png</frameFocus> 	
				</control>
				<control>
					<description>Filmstrip view</description>
					<type>filmstrip</type>
					<id>50</id>
					<dimColor>90ffffff</dimColor>
					<scrollOffset>2</scrollOffset>
					<onup>2</onup>
					<onleft>2</onleft>
					<posX>20</posX>
					<posY>410</posY>
					<width>1350</width>
					<spinCanFocus>no</spinCanFocus>
					<thumbWidth>150</thumbWidth>
					<thumbHeight>215</thumbHeight>
					<thumbPosX>21</thumbPosX>
					<thumbPosY>4</thumbPosY>
					<itemWidth>180</itemWidth>
					<itemHeight>280</itemHeight>
					<textureWidth>175</textureWidth>
					<textureHeight>223</textureHeight>
					<imageFolder flipY="true" diffuse="Thumb_Mask.png">DVDCase.png</imageFolder>
					<imageFolderFocus>DVDCase.png</imageFolderFocus>
					<frame>DVDCase.png</frame>
					<frameFocus>DVDCase.png</frameFocus>
					<background>emtpy.png</background>
					<thumbs flipY="true" diffuse="Thumb_Mask.png">
					</thumbs>
					<InfoImage flipY="true" diffuse="Thumb_Mask.png">
					</InfoImage>
					<showFrame>yes</showFrame>
					<showFolder>yes</showFolder>
					<showBackGround>no</showBackGround>
					<showInfoImage>no</showInfoImage>
					<visible>facadeview.filmstrip</visible>
					<thumbAnimation effect="slide" end="0,-22" time="200">focus</thumbAnimation>
					<thumbAnimation effect="zoom" start="100,100" end="120,120" time="200">focus</thumbAnimation>
					<scrollOffset>3</scrollOffset>
				</control>
				<control>
					<description>playlist listcontrol</description>
					<type>playlistcontrol</type>
					<id>50</id>
					<onleft>2</onleft>
					<onright>2</onright>
					<scrollOffset>2</scrollOffset>
					<posX>640</posX>
					<width>640</width>
					<posY>430</posY>
					<height>300</height>
					<textureHeight>35</textureHeight>
					<itemWidth>40</itemWidth>
					<itemHeight>30</itemHeight>
					<IconXOff>10</IconXOff>
					<IconYOff>2</IconYOff>
					<textXOff>10</textXOff>
					<textYOff2>2</textYOff2>
					<textYOff3>2</textYOff3>
					<font>font14</font>	
					<font2>font14</font2>	
					<font3>font14</font3>	
					<textYOff>2</textYOff>
					<spinWidth>30</spinWidth>
					<spinHeight>30</spinHeight>
					<spinPosX>1210</spinPosX>
					<spinPosY>685</spinPosY>
					<upBtnWidth>31</upBtnWidth>
					<downBtnWidth>31</downBtnWidth>
					<deleteBtnWidth>31</deleteBtnWidth>
					<upBtnHeight>34</upBtnHeight>
					<downBtnHeight>34</downBtnHeight>
					<deleteBtnHeight>34</deleteBtnHeight>
					<upBtnXOffset>540</upBtnXOffset>
					<downBtnXOffset>570</downBtnXOffset>
					<deleteBtnXOffset>600</deleteBtnXOffset>
					<upBtnYOffset>0</upBtnYOffset>
					<downBtnYOffset>0</downBtnYOffset>
					<deleteBtnYOffset>0</deleteBtnYOffset>

				</control>
			</control>
		</control>
	</controls>
</window>