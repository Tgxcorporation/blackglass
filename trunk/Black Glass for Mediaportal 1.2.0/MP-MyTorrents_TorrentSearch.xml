			</control>
			<control>
				<type>image</type>
				<posX>33</posX>
				<posY>47</posY>
				<width>1250</width>
				<height>53</height>
				<texture>bg_facade_line_nofocus.png</texture>
				<colordiffuse>77ffffff</colordiffuse>
			</control>
			<control>
				<description>Spin control Label</description>
				<type>fadelabel</type>
