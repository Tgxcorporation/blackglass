				<id>0</id>
				<posX>30</posX>
				<posY>10</posY>
				<label>#header.label</label>
				<font>font28</font>
				<align>left</align>
				<textcolor>60ffffff</textcolor>
				<visible>!control.hasfocus(4)</visible>
				<animation effect="fade" time="250">visiblechange</animation>
			</control>
			<control>
				<description>details background</description>
				<id>0</id>
				<type>image</type>
