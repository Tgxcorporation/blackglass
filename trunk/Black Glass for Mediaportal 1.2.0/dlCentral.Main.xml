<?xml version="1.0" encoding="utf-8"?>
<window>
	<controls>
		<!-- 
      This skin file is meant for all controls that should be visible for the user
      when in the main view of the plugin. All controls should be tagged at a minimum
      with "<visible>Control.isvisible(101)</visible>". 
      -->

		<control>
			<type>group</type>
			<dimColor>ffffffff</dimColor>
			<visible>Control.isvisible(101)</visible>
			<animation effect="fade" time="250">visiblechange</animation>
			<animation effect="fade" time="150">WindowClose</animation>	
			<animation effect="slide" tween="circle" easing="out" time="200" start="900,0" reversible="false">WindowOpen</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" end="900,0" reversible="false">WindowClose</animation>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>458</posX>
				<posY>48</posY>
				<width>948</width>
				<height>766</height>
				<texture>bg_facade.png</texture>
			</control>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>490</posX>
				<posY>97</posY>
				<width>790</width>
				<height>53</height>
				<texture>bg_facade_line_nofocus.png</texture>
				<colordiffuse>77ffffff</colordiffuse>
			</control>


			<control>
				<type>fadelabel</type>
				<id>0</id>
				<posX>520</posX>
				<posY>95</posY>
				<font>font24</font>
				<align>left</align>
				<textcolor>ffffffff</textcolor>
				<width>750</width>
				<label>#header.label</label>
				<wrapString> : : : </wrapString>
			</control>
			<control>
				<type>textbox</type>
				<id>912</id>
				<posX>520</posX>
				<posY>163</posY>
				<width>750</width>
				<height>520</height>
				<label>#dlCentral.About</label>
				<font>font16</font>
				<textcolor>90ffffff</textcolor>
				<onup>912</onup>
				<ondown>912</ondown>
				<onleft>202</onleft>
				<onright>50</onright>
				<spinWidth>30</spinWidth>
				<spinWidth>40</spinWidth>
				<spinHeight>40</spinHeight>
				<spinPosX>1135</spinPosX>
				<spinPosY>660</spinPosY>
				<spinAlign>Right</spinAlign>
				<spinCanFocus>yes</spinCanFocus>
			</control>



			<control>
				<type>image</type>
				<id>0</id>
				<posX>490</posX>
				<posY>582</posY>
				<width>790</width>
				<height>53</height>
				<texture>bg_facade_line_nofocus.png</texture>
				<colordiffuse>77ffffff</colordiffuse>
			</control>


		</control>

		<control>
			<description>Dummy Button</description>
			<type>button</type>
			<id>500</id>
			<label>-</label>
			<onup>500</onup>
			<ondown>500</ondown>
			<onleft>202</onleft>
			<onright>50</onright>
			<textureFocus>-</textureFocus>
			<textureNoFocus>-</textureNoFocus>
			<!--<visible>Control.isvisible(101)</visible>-->
			<visible>no</visible>
		</control>






	</controls>
</window>
