			<font>font16</font>
			<align>right</align>
			<textcolor>ffffffff</textcolor>
			<animation effect="fade" time="250">WindowClose</animation>	
			<animation effect="fade" time="250">WindowOpen</animation>
		</control>
		<control>
			<description>sms input</description>
			<type>smsinput</type>
			<id>51</id>
			<posX>950</posX>
			<posY>475</posY>
			<width>300</width>
			<height>100</height>
			<onleft>51</onleft>
			<onright>51</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<font>font13</font>
			<textcolor2>ffAAAAAA</textcolor2>
			<textboxFont>font14</textboxFont> 
			<textboxXpos>40</textboxXpos>
			<textboxYpos>550</textboxYpos>
			<textboxWidth>900</textboxWidth>
			<textboxHeight>88</textboxHeight>
			<textboxColor>ffffffff</textboxColor>
			<textboxBgColor>ffffffff</textboxBgColor>
			<animation effect="fade" time="250">WindowClose</animation>	
			<animation effect="fade" time="250">WindowOpen</animation>
		</control>
	</controls>
