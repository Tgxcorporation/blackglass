<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
	<controls>
		<!-- these elements need condition visibility for OSD IS VISIBLE -->
		<control>
			<description>background up</description>
			<id>300</id>
			<type>image</type>
			<posX>-10</posX>
			<posY>0</posY>
			<width>1300</width>
			<height>36</height>
			<texture>osd_progress_background2.png</texture>
			<animation effect="fade" time="100">visible</animation>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+!window.isosdvisible</visible>
		</control>

		<control>
			<description>Movie Name</description>
			<id>36</id>
			<type>fadelabel</type>
			<width>1100</width>
			<height>36</height>
			<posX>10</posX>
			<posY>5</posY>
			<font>font14</font>
			<label>#Play.Current.Title</label>
			<textcolor>white</textcolor>
			<align>left</align>
			<animation effect="fade" time="100">visible</animation>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+!window.isosdvisible</visible>
			<wrapString> : : : </wrapString>
			<shadowAngle>45</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>
		<control>
			<description>Current Time Info</description>
			<type>label</type>
			<posX>1240</posX>
			<posY>5</posY>
			<label>#time</label>
			<font>font14</font>
			<textcolor>white</textcolor>
			<align>right</align>
			<animation effect="fade" time="100">visible</animation>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+!window.isosdvisible</visible>
			<shadowAngle>45</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>
		<control>
			<description>time logo</description>
			<type>image</type>
			<posX>1248</posX>
			<posY>6</posY>
			<width>24</width>
			<height>24</height>
			<texture>time.png</texture>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+!window.isosdvisible</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>
		<control>
			<description>progress bar background</description>
			<type>image</type>
			<posX>-10</posX>
			<posY>685</posY>
			<width>1300</width>
			<height>36</height>
			<texture>osd_progress_background.png</texture>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>
		<control>
			<description>progress bar</description>
			<type>progress</type>
			<id>1</id>
			<posX>-12</posX>
			<posY>685</posY>
			<width>1304</width>
			<height>36</height>
			<label>#play.progressbar.percentage</label>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>

		<control>
			<description>Start Time Info</description>
			<type>label</type>
			<posX>10</posX>
			<posY>687</posY>
			<label>#play.progressbar.currentplaytime</label>
			<align>left</align>
			<font>font16</font>
			<textcolor>white</textcolor>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
			<shadowAngle>315</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>

		<control>
			<description>End Time Info</description>
			<type>label</type>
			<posX>1270</posX>
			<posY>687</posY>
			<label>#play.progressbar.currentremaining</label>
			<align>right</align>
			<font>font16</font>
			<textcolor>white</textcolor>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
			<shadowAngle>315</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>

		<control>
			<description>Time</description>
			<type>label</type>
			<id>0</id>
			<posX>655</posX>
			<posY>687</posY>
			<label>#time</label>
			<align>right</align>
			<font>font16</font>
			<textcolor>ffffffff</textcolor>
			<visible>![player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+window.isosdvisible</visible>
			<animation effect="fade" time="250">visiblechange</animation>
			<animation effect="fade" time="250">windowopen</animation>
			<shadowAngle>315</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>
		<control>
			<description>time logo</description>
			<type>image</type>
			<id>999</id>
			<posX>665</posX>
			<posY>690</posY>
			<width>23</width>
			<height>23</height>
			<texture>time.png</texture>
			<visible>![player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+window.isosdvisible</visible>
			<animation effect="fade" time="250">visiblechange</animation>
			<animation effect="fade" time="250">windowopen</animation>
		</control>

		<!-- END OF these elements need condition visibility for OSD IS VISIBLE -->
