		<control>
			<description>BG</description>
			<type>image</type>
			<id>0</id>
			<width>1920</width>
			<height>1080</height>
			<texture>#header.background</texture>
			<animation effect="fade" time="1">WindowClose</animation>
		</control>
		<control>
			<type>fadelabel</type>
