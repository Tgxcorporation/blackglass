<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
	<id>1987</id>
	<defaultcontrol>50</defaultcontrol>
	<allowoverlay>player.hasmedia</allowoverlay><define>#header.label:Video Search</define>
	<define>#header.background:bg.jpg</define>
	<animation effect="fade" time="250" start="100" end="100">WindowClose</animation>
	<controls>
		<import>common.window.background.xml</import>
		<import>common.window.overlay.xml</import>
		<import>common.window.time.xml</import>
		<control>
			<type>group</type>
			<dimColor>#ColorMain</dimColor>
			<animation effect="fade" time="150">WindowClose</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0" reversible="false">WindowOpen</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" end="1350,0" reversible="false">WindowClose</animation>
			<animation effect="slide" tween="sine" easing="in" time="200" end="291,0" condition="!control.hasfocus(50)">conditional</animation>
			<control>
				<type>label</type>
				<id>0</id>
				<posX>45</posX>
				<posY>15</posY>
				<label>#header.label</label>
				<font>font22</font>
				<align>left</align>
				<textcolor>#ColorUnfocused</textcolor>
			</control>
			<control>
				<description>Status</description>
				<type>fadelabel</type>
				<id>6</id>
				<posX>45</posX>
				<posY>66</posY>
				<font>font13</font>
				<textcolor>#ColorMain</textcolor>
				<wrapString> : : : </wrapString>
			</control>
			<control>
				<id>500</id>
				<type>image</type>
				<posX>60</posX>
				<posY>118</posY>
				<width>511</width>
				<height>381</height>
				<keepaspectratio>yes</keepaspectratio>
				<texture flipY="true" diffuse="Thumb_Mask.png">#selectedthumb</texture>
				<visible>[facadeview.smallicons|facadeview.list|facadeview.largeicons|facadeview.album|facadeview.playlist]</visible>
				<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0">visible</animation>
				<centered>yes</centered>
			</control>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>45</posX>
				<posY>109</posY>
				<width>540</width>
				<height>409</height>
				<texture flipY="true" diffuse="Thumb_Mask.png">PictureCase.png</texture>
				<visible>[facadeview.smallicons|facadeview.list|facadeview.largeicons|facadeview.album|facadeview.playlist]</visible>
				<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0">visible</animation>
			</control>
			<control>
				<type>fadelabel</type>
				<description>Current program title</description>
				<id>0</id>
				<posX>45</posX>
				<posY>532</posY>
				<width>540</width>
				<font>font22</font>
				<label>#selecteditem</label>
				<visible>[facadeview.smallicons|facadeview.list|facadeview.largeicons|facadeview.album|facadeview.playlist]</visible>
				<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0">visible</animation>
				<wrapString> : : : </wrapString>
			</control>
		</control>
		<control>
			<id>0</id>
			<type>image</type>
			<posX>-300</posX>
			<posY>600</posY>
			<width>2520</width>
			<height>480</height>
			<texture>gradient.png</texture>
			<visible>[facadeview.coverflow|facadeview.filmstrip]+control.isvisible(50)</visible>
			<animation effect="fade" time="250">visiblechange</animation>
			<animation effect="fade" time="150">WindowOpen</animation>
			<animation effect="fade" time="150">WindowClose</animation>
		</control>
		<control>
			<description>group element</description>
			<type>group</type>
			<dimColor>#ColorMain</dimColor>
			<animation effect="fade" time="150">WindowClose</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0" reversible="false">WindowOpen</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" end="1350,0" reversible="false">WindowClose</animation>
			<animation effect="slide" tween="sine" easing="in" time="200" end="288,0" condition="!control.hasfocus(50)">conditional</animation>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>687</posX>
				<posY>72</posY>
				<width>1422</width>
				<height>1149</height>
				<texture>bg_facade.png</texture>
				<visible>facadeview.list+control.isvisible(50)</visible>
				<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0">visible</animation>
			</control>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>735</posX>
				<posY>71</posY>
				<width>1185</width>
				<height>79</height>
				<texture>bg_facade_line_nofocus.png</texture>
				<colordiffuse>77ffffff</colordiffuse>
				<visible>facadeview.list+control.isvisible(50)+[!string.equals(#itemcount,)+!string.equals(#itemcount,0)]</visible>
				<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0">visible</animation>
			</control>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>687</posX>
				<posY>72</posY>
				<width>1422</width>
				<height>1149</height>
				<texture>bg_facade.png</texture>
				<visible>facadeview.smallicons+control.isvisible(50)</visible>
				<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0">visible</animation>
			</control>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>687</posX>
				<posY>72</posY>
				<width>1422</width>
				<height>1149</height>
				<texture>bg_facade.png</texture>
				<visible>facadeview.largeicons+control.isvisible(50)</visible>
				<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0">visible</animation>
			</control>
			<control>
				<description>composite control consisting of a list control and a thumbnail panel</description>
				<type>facadeview</type>
				<id>50</id>
				<control>
					<description>listcontrol</description>
					<type>listcontrol</type>
					<id>50</id>
					<onleft>102</onleft>
					<onright>102</onright>
					<scrollOffset>2</scrollOffset>
					<visible>facadeview.list</visible>
					<animation effect="slide" tween="circle" easing="out" time="200" start="600,0">visible</animation>
				</control>
				<control>
					<description>Thumbnail Panel</description>
					<type>thumbnailpanel</type>
					<id>50</id>
					<onleft>102</onleft>
					<onright>102</onright>
					<posX>750</posX>
					<posY>142</posY>
					<width>1222</width>
					<height>825</height>
					<scrollOffset>1</scrollOffset>
					<itemWidth>145</itemWidth>
					<itemHeight>135</itemHeight>
					<textureWidth>124</textureWidth>
					<textureHeight>94</textureHeight>
					<thumbWidth>115</thumbWidth>
					<thumbHeight>84</thumbHeight>
					<thumbPosX>4</thumbPosX>
					<thumbPosY>4</thumbPosY>
					<itemWidthBig>287</itemWidthBig>
					<itemHeightBig>271</itemHeightBig>
					<thumbWidthBig>265</thumbWidthBig>
					<thumbHeightBig>199</thumbHeightBig>
					<textureWidthBig>283</textureWidthBig>
					<textureHeightBig>214</textureHeightBig>
					<thumbPosXBig>9</thumbPosXBig>
					<thumbPosYBig>7</thumbPosYBig>
					<unfocusedAlpha>120</unfocusedAlpha>
					<hideUnfocusTexture>no</hideUnfocusTexture>
					<renderFocusText>yes</renderFocusText>
					<renderUnfocusText>yes</renderUnfocusText>
					<imageFolder>-</imageFolder>
					<imageFolderFocus>-</imageFolderFocus>
					<thumbZoom>yes</thumbZoom>
					<enableFocusZoom>no</enableFocusZoom>
					<textXOff>3</textXOff>
					<textYOff>3</textYOff>
					<spinCanFocus>no</spinCanFocus>
					<showFrame>true</showFrame>
					<frameNoFocus>PictureCase.png</frameNoFocus>
					<frameFocus>PictureCase.png</frameFocus>
					<visible>facadeview.largeicons|facadeview.smallicons</visible>
					<animation effect="slide" tween="circle" easing="out" time="200" start="600,0">visible</animation>
				</control>
			</control>
		</control>
		<control>
			<type>actiongroup</type>
			<description>action menu</description>
			<defaultcontrol>102</defaultcontrol>
			<onexit>0</onexit>
			<buttonX>0</buttonX>
			<buttonY>0</buttonY>
			<buttonwidth>577</buttonwidth>
			<buttonheight>1080</buttonheight>
			<textureFocus>-</textureFocus>
			<textureNoFocus>-</textureNoFocus>
			<width>637</width>
			<height>1080</height>
			<dimColor>#ColorMain</dimColor>
			<animation effect="slide" tween="circle" easing="out" time="250" start="-289,0" reversible="false" condition="!control.hasfocus(50)+!control.hasfocus(505050)">conditional</animation>
			<animation effect="slide" tween="circle" easing="out" time="250" end="-289,0" reversible="false" condition="control.hasfocus(50)|control.hasfocus(505050)">conditional</animation>
			<animation effect="slide" time="150" end="-600,0" reversible="false">WindowClose</animation>
			<animation effect="slide" tween="circle" easing="out" time="250" start="-312,0" reversible="false">WindowOpen</animation>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>0</posX>
				<posY>0</posY>
				<width>627</width>
				<height>1080</height>
				<texture>bg_left.png</texture>
				<colorDiffuse>#ColorMain</colorDiffuse>
			</control>
			<control>
				<id>0</id>
				<type>label</type>
				<posX>45</posX>
				<posY>52</posY>
				<label>924</label>
				<width>499</width>
				<font>font24</font>
				<align>left</align>
				<textcolor>#ColorMain</textcolor>
			</control>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>0</posX>
				<posY>71</posY>
				<width>579</width>
				<height>79</height>
				<texture>bg_button_nofocus.png</texture>
			</control>
			<control>
				<type>group</type>
				<layout>StackLayout(0, Vertical, true)</layout>
				<posX>0</posX>
				<posY>150</posY>
				<control>
					<type>button</type>
					<description>Search</description>
					<id>102</id>
					<onright>50</onright>
					<label>Search</label>
					<onup>101</onup>
					<ondown>101</ondown>
				</control>
				<control>
					<description>Top Rated</description>
					<type>button</type>
					<id>101</id>
					<onright>50</onright>
					<label>Top Rated</label>
					<onup>102</onup>
					<ondown>102</ondown>
				</control>
			</control>
		</control>
	</controls>
</window>
