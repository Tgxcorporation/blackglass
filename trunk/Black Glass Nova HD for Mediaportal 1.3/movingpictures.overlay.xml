    <control>
      <type>image</type>
      <id>0</id>
      <width>474</width>
      <height>174</height>
      <posX>19</posX>
      <posY>120</posY>
      <texture>bg_button2_focus.png</texture>
      <colordiffuse>ccffffff</colordiffuse>
      <visible>player.hasmedia+control.isvisible(3337)</visible>
      <animation effect="fade" time="250">visiblechange</animation>
    </control>
