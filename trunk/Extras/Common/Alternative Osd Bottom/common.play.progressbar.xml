<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
	<controls>
		<!-- these elements need condition visibility for OSD IS VISIBLE -->

		<control>
			<description>progressbar background</description>
			<id>300</id>
			<type>image</type>
			<posX>149</posX>
			<posY>580</posY>
			<width>982</width>
			<height>136</height>
			<texture>bg_osd_small_top.png</texture>
			<animation effect="fade" time="100">visible</animation>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+!window.isosdvisible</visible>
		</control>
		<control>
			<description>progressbar background</description>
			<id>999</id>
			<type>image</type>
			<posX>180</posX>
			<posY>653</posY>
			<height>29</height>
			<width>920</width>
			<texture>bg_osd_small_progress_background.png</texture>
			<animation effect="fade" time="100">visible</animation>
			<visible>[window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)]</visible>
		</control>
		<control>
			<description>progress bar</description>
			<type>progress</type>
			<id>996</id>
			<posX>168</posX>
			<posY>653</posY>
			<height>29</height>
			<width>921</width>
			<texturebg>-</texturebg>
			<lefttexture>osd_small_progress_left_blue.png</lefttexture>
			<midtexture>osd_small_progress_mid_blue.png</midtexture>
			<righttexture>-</righttexture>
			<label>#play.progressbar.percentage</label>
			<visible>[window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)]</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>
		<control>
			<description>Movie Name</description>
			<id>36</id>
			<type>fadelabel</type>
			<width>740</width>
			<posX>200</posX>
			<posY>618</posY>
			<font>font18</font>
			<label>#Play.Current.Title</label>
			<textcolor>white</textcolor>
			<textalign>left</textalign>
			<wrapString> : : : </wrapString>
			<animation effect="fade" time="100">visible</animation>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+!window.isosdvisible</visible>
		</control>
		<control>
			<type>image</type>
			<description>Time icon</description>
			<id>999</id>
			<posX>1060</posX>
			<posY>624</posY>
			<height>23</height>
			<width>23</width>
			<texture>time.png</texture>
			<colordiffuse>77ffffff</colordiffuse>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+!window.isosdvisible</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>

		<control>
			<type>fadelabel</type>
			<description>Time</description>
			<id>999</id>
			<posX>1058</posX>
			<posY>625</posY>
			<width>300</width>
			<font>font12</font>
			<align>right</align>
			<label>#time</label>
			<textcolor>47ffffff</textcolor>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+!window.isosdvisible</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>
		
		<control>
			<type>image</type>
			<description>Time icon</description>
			<id>999</id>
			<posX>654</posX>
			<posY>656</posY>
			<height>23</height>
			<width>23</width>
			<texture>time.png</texture>
			<colordiffuse>ffffffff</colordiffuse>
			<visible>![player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+window.isosdvisible</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>

		<control>
			<type>label</type>
			<description>Time</description>
			<id>999</id>
			<posX>652</posX>
			<posY>657</posY>
			<font>font12</font>
			<align>right</align>
			<label>#time</label>
			<textcolor>ffffffff</textcolor>
			<visible>![player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]+window.isosdvisible</visible>
			<animation effect="fade" time="100">visible</animation>
			<shadowAngle>315</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>
		<control>
			<description>Start Time Info</description>
			<id>999</id>
			<type>label</type>
			<posX>200</posX>
			<posY>654</posY>
			<label>#play.progressbar.currentplaytime</label>
			<align>left</align>
			<font>font14</font>
			<textcolor>white</textcolor>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
			<shadowAngle>315</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>
		<control>
			<description>End Time Info</description>
			<id>999</id>
			<type>label</type>
			<posX>1080</posX>
			<posY>654</posY>
			<label>#play.progressbar.currentremaining</label>
			<align>right</align>
			<font>font14</font>
			<textcolor>white</textcolor>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
			<shadowAngle>315</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>






