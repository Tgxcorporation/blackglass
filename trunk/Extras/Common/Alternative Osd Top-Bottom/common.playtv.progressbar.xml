<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
	<controls>
		<!-- these elements need condition visibility for OSD IS VISIBLE -->

		<control>
			<description>progressbar background</description>
			<id>300</id>
			<type>image</type>
			<posX>149</posX>
			<posY>30</posY>
			<width>982</width>
			<height>136</height>
			<texture>bg_osd_small_top.png</texture>
			<animation effect="fade" time="100">visible</animation>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]|window.isosdvisible</visible>
		</control>

		<control>
			<description>TV Progress Bar</description>
			<type>tvprogress</type>
			<id>993</id>
			<posX>180</posX>
			<posY>103</posY>
			<height>29</height>
			<width>897</width>
			<toptexture>-</toptexture>   
			<TextureOffsetY>18</TextureOffsetY>
			<bottomtexture>-</bottomtexture>
			<texturetick>-</texturetick>
			<lefttexture>osd_small_progress_left_blue.png</lefttexture>
			<midtexture>-</midtexture>
			<righttexture>-</righttexture>
			<logotexture>-</logotexture>
			<fillbackgroundtexture>-</fillbackgroundtexture>
			<fillbgxoff>-3</fillbgxoff>
			<fillbgyoff>0</fillbgyoff>
			<filltexture1>osd_small_progress_mid_blue.png</filltexture1>
			<filltexture2>osd_small_progress_mid_red.png</filltexture2>
			<filltexture3>osd_small_progress_mid_green.png</filltexture3>
			<fillheight>29</fillheight>
			<label>#TV.Record.percent1</label>
			<label1>#TV.Record.percent2</label1>
			<label2>#TV.Record.percent3</label2>
			<font>font13</font>
			<textcolor>FF000000</textcolor>
			<visible>[window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)]</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>







		<control>
			<description>channelname</description>
			<id>14</id>
			<type>fadelabel</type>
			<width>740</width>
			<posX>200</posX>
			<posY>68</posY>
			<font>font18</font>
			<label>#TV.View.channel - #TV.View.title</label>
			<align>left</align>
			<textcolor>White</textcolor>
			<wrapString> : : : </wrapString>
			<animation effect="fade" time="100">visible</animation>
			<visible>[player.rewinding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)|player.forwarding|player.paused]|window.isosdvisible</visible>
		</control>


		<control>
			<type>image</type>
			<description>Time icon</description>
			<id>999</id>
			<posX>1060</posX>
			<posY>75</posY>
			<height>22</height>
			<width>22</width>
			<texture>time.png</texture>
			<colordiffuse>77ffffff</colordiffuse>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>

		<control>
			<type>fadelabel</type>
			<description>Time</description>
			<id>999</id>
			<posX>1058</posX>
			<posY>75</posY>
			<width>300</width>
			<font>font12</font>
			<align>right</align>
			<label>#time</label>
			<textcolor>77ffffff</textcolor>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
		</control>





		<control>
			<description>Start Time Info</description>
			<id>999</id>
			<type>label</type>
			<posX>200</posX>
			<posY>104</posY>
			<label>#play.progressbar.currentplaytime</label>
			<align>left</align>
			<font>font14</font>
			<textcolor>white</textcolor>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
			<shadowAngle>315</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>
		<control>
			<description>End Time Info</description>
			<id>999</id>
			<type>label</type>
			<posX>1080</posX>
			<posY>104</posY>
			<label>#play.progressbar.currentremaining</label>
			<align>right</align>
			<font>font14</font>
			<textcolor>white</textcolor>
			<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|control.isvisible(10)|control.isvisible(11)|control.isvisible(12)</visible>
			<animation effect="fade" time="100">visible</animation>
			<shadowAngle>315</shadowAngle>
			<shadowDistance>1</shadowDistance>
			<shadowColor>FF000000</shadowColor>
		</control>
		
	


		<!-- END OF these elements need condition visibility for OSD IS VISIBLE -->
