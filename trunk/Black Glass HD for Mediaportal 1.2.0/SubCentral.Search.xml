<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8"-->
<window>
  <controls>
    <!-- 
		This skin file is meant for all controls that should be visible for the user
		when in the search view of the plugin. All controls should be tagged at a minimum
		with "<visible>Control.isvisible(102)</visible>". 
		-->
    <control>
      <type>group</type>
      <dimColor>ffffffff</dimColor>
      <visible>Control.isvisible(102)</visible>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="150">WindowClose</animation>
      <animation effect="slide" tween="circle" easing="out" time="200" start="1350,0" reversible="false">WindowOpen</animation>
      <animation effect="slide" tween="circle" easing="out" time="200" end="1350,0" reversible="false">WindowClose</animation>
      <control>
        <type>image</type>
        <id>0</id>
        <posX>687</posX>
        <posY>72</posY>
        <width>1422</width>
        <height>1149</height>
        <texture>bg_facade.png</texture>
      </control>
      <control>
        <type>image</type>
        <id>0</id>
        <posX>735</posX>
        <posY>71</posY>
        <width>1185</width>
        <height>79</height>
        <texture>bg_facade_line_nofocus.png</texture>
        <colordiffuse>77ffffff</colordiffuse>
      </control>
      <control>
        <type>label</type>
        <description>Number of Files Label</description>
        <id>0</id>
        <posX>765</posX>
        <posY>989</posY>
        <font>font16</font>
        <label>#itemcount</label>
        <align>left</align>
        <textcolor>60ffffff</textcolor>
      </control>
      <control>
        <description>group element</description>
        <type>group</type>
        <dimColor>ffffffff</dimColor>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <onleft>202</onleft>
          <onright>202</onright>
          <scrollOffset>2</scrollOffset>
          <font>font14</font>
          <font2>font11</font2>
          <font3>font11</font3>
          <textYOff>4</textYOff>
          <textXOff>15</textXOff>
          <textXOff2>0</textXOff2>
          <textYOff2>45</textYOff2>
          <textXOff3>105</textXOff3>
          <textYOff3>45</textYOff3>
        </control>
      </control>
    </control>
    <control>
      <type>group</type>
      <dimColor>ffffffff</dimColor>
      <animation effect="fade" time="150">WindowClose</animation>
      <animation effect="slide" tween="circle" easing="out" time="200" start="1350,0" reversible="false">WindowOpen</animation>
      <animation effect="slide" tween="circle" easing="out" time="200" end="1350,0" reversible="false">WindowClose</animation>
      <control>
        <description>Cover Art</description>
        <type>image</type>
        <id>911</id>
        <keepaspectratio>no</keepaspectratio>
        <posX>93</posX>
        <posY>132</posY>
        <width>367</width>
        <height>522</height>
        <texture flipY="true" diffuse="Thumb_Mask.png">#SubCentral.Search.Media.Thumb</texture>
        <visible>Control.isvisible(102)+ control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <description>Cover Art Glass Overlay</description>
        <type>image</type>
        <id>0</id>
        <posX>45</posX>
        <posY>124</posY>
        <width>424</width>
        <height>540</height>
        <texture flipY="true" diffuse="Thumb_Mask.png">DVDCase.png</texture>
        <visible>Control.isvisible(102)+ control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>fadelabel</type>
        <id>0</id>
        <posX>45</posX>
        <posY>690</posY>
        <width>630</width>
        <label>#SubCentral.Search.Media.Title</label>
        <font>font22</font>
        <wrapString> : : : </wrapString>
        <visible>Control.isvisible(102)+String.Equals(#SubCentral.Search.SearchType.Type,NONE)+ control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>fadelabel</type>
        <id>0</id>
        <posX>45</posX>
        <posY>690</posY>
        <width>630</width>
        <label>#SubCentral.Search.Media.TitleWithYear</label>
        <font>font22</font>
        <wrapString> : : : </wrapString>
        <visible>Control.isvisible(102)+[String.Equals(#SubCentral.Search.SearchType.Type,MOVIE)|String.Equals(#SubCentral.Search.SearchType.Type,IMDb)]+ control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>fadelabel</type>
        <id>0</id>
        <posX>45</posX>
        <posY>690</posY>
        <width>630</width>
        <label>#SubCentral.Search.Media.TitleWithSE</label>
        <font>font22</font>
        <wrapString> : : : </wrapString>
        <visible>Control.isvisible(102)+String.Equals(#SubCentral.Search.SearchType.Type,TVSHOW)+ control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>fadelabel</type>
        <id>0</id>
        <posX>45</posX>
        <posY>757</posY>
        <width>630</width>
        <font>font16</font>
        <label>#SubCentral.Search.Source.Text</label>
        <wrapString> : : : </wrapString>
        <textcolor>90ffffff</textcolor>
        <visible>Control.isvisible(102)+ control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>fadelabel</type>
        <id>0</id>
        <posX>45</posX>
        <posY>810</posY>
        <width>630</width>
        <label>#SubCentral.Search.SearchType.Text</label>
        <font>font16</font>
        <wrapString> : : : </wrapString>
        <visible>Control.isvisible(102)+!String.Equals(#SubCentral.Search.SearchType.Type,NONE)+ control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>fadelabel</type>
        <id>0</id>
        <posX>45</posX>
        <posY>810</posY>
        <width>630</width>
        <label>#SubCentral.Search.SearchType.Text</label>
        <font>font16</font>
        <wrapString> : : : </wrapString>
        <visible>Control.isvisible(102)+String.Equals(#SubCentral.Search.SearchType.Type,NONE)+ control.hasfocus(50)</visible>
        <textcolor>ffF87B72</textcolor>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>fadelabel</type>
        <id>0</id>
        <posX>45</posX>
        <posY>862</posY>
        <width>630</width>
        <label>#SubCentral.Search.Media.IMDb.ID.Text</label>
        <font>font16</font>
        <wrapString> : : : </wrapString>
        <visible>Control.isvisible(102)+String.Equals(#SubCentral.Search.SearchType.Type,IMDb)+ control.hasfocus(50)</visible>
        <textcolor>90ffffff</textcolor>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <description>Media Files</description>
        <type>textboxscrollup</type>
        <id>0</id>
        <posX>45</posX>
        <posY>915</posY>
        <width>630</width>
        <height>135</height>
        <colordiffuse>ffffffff</colordiffuse>
        <font>font14</font>
        <label>#SubCentral.Search.Files.AllNames</label>
        <seperator>: : :</seperator>
        <visible>Control.isvisible(102)+ control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
    </control>
  </controls>
</window>