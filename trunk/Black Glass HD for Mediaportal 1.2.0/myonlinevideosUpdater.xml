    <import>common.window.time.xml</import>
    <import>common.facade.background.basic.xml</import>
    <import>common.facade.itemcount.itemtype.basic.xml</import>
    <control>
      <type>group</type>
      <animation effect="slide" tween="circle" easing="out" time="200" start="1350,0" reversible="false">WindowOpen</animation>
      <animation effect="slide" tween="circle" easing="out" time="200" end="1350,0" reversible="false">WindowClose</animation>
      <control>
        <description>listcontrol</description>
        <type>listcontrol</type>
        <id>50</id>
        <scrollOffset>2</scrollOffset>
        <onleft>505</onleft>
        <textYOff>15</textYOff>
        <textXOff>15</textXOff>
        <textXOff2>712</textXOff2>
        <textYOff2>15</textYOff2>
        <textXOff3>765</textXOff3>
        <textYOff3>18</textYOff3>
      </control>
    </control>
    <control>
      <type>group</type>
      <dimColor>ffffffff</dimColor>
      <animation effect="fade" time="150">WindowClose</animation>
