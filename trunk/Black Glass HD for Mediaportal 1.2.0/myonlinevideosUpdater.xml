        <align>left</align>
        <textcolor>ffffffff</textcolor>
      </control>
      <control>
        <type>image</type>
        <id>0</id>
        <posX>0</posX>
        <posY>71</posY>
        <width>579</width>
        <height>79</height>
        <texture>bg_button_nofocus.png</texture>
      </control>
      <control>
        <type>group</type>
        <layout>StackLayout(0, Vertical, true)</layout>
        <posX>0</posX>
        <posY>150</posY>
        <control>
          <type>button</type>
          <description>full update button</description>
          <id>505</id>
          <onright>50</onright>
          <onup>17</onup>
          <ondown>504</ondown>
          <label>#OnlineVideos.Translation.FullUpdate.Label</label>
          <visible allowhiddenfocus="true">!Control.HasFocus(50)</visible>
        </control>
        <control>
          <type>selectbutton</type>
          <description>Order By</description>
          <id>504</id>
          <onright>50</onright>
          <onup>505</onup>
          <ondown>503</ondown>
          <label>#OnlineVideos.Translation.SortOptions.Label</label>
          <visible>!Control.HasFocus(50)</visible>
        </control>
        <control>
          <type>selectbutton</type>
          <description>filter button</description>
          <id>503</id>
          <onright>50</onright>
          <onup>504</onup>
          <ondown>506</ondown>
          <label>#OnlineVideos.Translation.Filter.Label: #OnlineVideos.Translation.State.Label</label>
          <visible>!Control.HasFocus(50)</visible>
        </control>
        <control>
          <type>selectbutton</type>
          <description>filter button for Creator</description>
          <id>506</id>
          <onright>50</onright>
          <onup>503</onup>
          <ondown>507</ondown>
