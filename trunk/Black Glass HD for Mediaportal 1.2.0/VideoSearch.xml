				<posY>52</posY>
				<label>#header.label</label>
				<width>500</width>
				<font>font24</font>
				<align>left</align>
				<textcolor>ffffffff</textcolor>
			</control>
			<control>
				<type>image</type>
				<id>0</id>
				<posX>0</posX>
				<posY>71</posY>
				<width>579</width>
				<height>79</height>
				<texture>bg_button_nofocus.png</texture>
