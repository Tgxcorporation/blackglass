	<allowoverlay>player.hasmedia+[facadeview.smallicons|facadeview.list|facadeview.largeicons|facadeview.album|facadeview.playlist]</allowoverlay>
	<define>#header.label:Video Search</define>
	<define>#header.background:bg.jpg</define>
	<animation effect="fade" time="250" start="100" end="0">WindowClose</animation>
	<animation effect="fade" time="250" start="0" end="100">WindowOpen</animation>
	<controls>
		<import>common.window.background.xml</import>
		<import>common.window.overlay.xml</import>
		<import>common.window.time.xml</import>
		<import>common.facadeview.background.xml</import>
		<control>
			<type>group</type>
			<dimColor>ffffffff</dimColor>
			<animation effect="fade" time="150">WindowClose</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" start="1350,0" reversible="false">WindowOpen</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" end="1350,0" reversible="false">WindowClose</animation>
			<control>
				<type>label</type>
				<id>0</id>
				<posX>45</posX>
				<posY>15</posY>
				<label>#header.label</label>
				<font>font22</font>
				<align>left</align>
				<textcolor>60ffffff</textcolor>
				<visible>![control.hasfocus(102)|control.hasfocus(101)]</visible>
				<animation effect="fade" time="250">visiblechange</animation>
			</control>
			<control>
				<description>Status</description>
				<type>fadelabel</type>
				<id>6</id>
				<posX>45</posX>
				<posY>66</posY>
				<font>font13</font>
				<textcolor>ffffffff</textcolor>
				<wrapString> : : : </wrapString>
				<visible>![control.hasfocus(102)|control.hasfocus(101)]</visible>
				<animation effect="fade" time="250">visiblechange</animation>
			</control>
			<control>
				<type>label</type>
				<description>Title (Filmstrip)</description>
				<id>640</id>
				<posX>57</posX>
				<posY>1015</posY>
				<width>1830</width>
				<align>center</align>
				<font>font24</font>
				<label>#selecteditem</label>
				<visible>[facadeview.filmstrip]</visible>
				<animation effect="fade" time="250">visiblechange</animation>
			</control>
