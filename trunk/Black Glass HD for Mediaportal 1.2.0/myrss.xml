<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>2700</id>
  <defaultcontrol>50</defaultcontrol>
  <allowoverlay>player.hasmedia+[facadeview.smallicons|facadeview.list|facadeview.largeicons|facadeview.album|facadeview.playlist]</allowoverlay>
  <define>#header.label:9</define>
  <define>#header.background:hover_my news.png</define>
  <animation effect="fade" time="250" start="100" end="100">WindowClose</animation>
  <controls>
    <import>common.window.background.xml</import>
    <import>common.window.overlay.xml</import>
    <import>common.window.accessories.xml</import>
    <import>common.window.time.xml</import>
    <import>common.facade.background.basic.xml</import>
    <import>common.facade.itemcount.currentmodule.basic.xml</import>
    <control>
      <type>group</type>
      <dimColor>ffffffff</dimColor>
      <animation effect="fade" time="150">WindowClose</animation>
      <animation effect="slide" tween="circle" easing="out" time="200" start="1350,0" reversible="false">WindowOpen</animation>
      <animation effect="slide" tween="circle" easing="out" time="200" end="1350,0" reversible="false">WindowClose</animation>
      <control>
        <type>label</type>
        <id>0</id>
        <posX>45</posX>
        <posY>15</posY>
        <label>#header.label</label>
        <font>font28</font>
        <align>left</align>
        <textcolor>60ffffff</textcolor>
        <visible>control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>textboxscrollup</type>
        <description>Chosen Story description</description>
        <id>506</id>
        <posX>45</posX>
        <posY>195</posY>
        <width>525</width>
        <height>645</height>
        <colordiffuse>ffffffff</colordiffuse>
        <font>font16</font>
        <label>-</label>
        <visible>control.hasfocus(50)</visible>
        <seperator>: : :</seperator>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <description>Site Logo</description>
        <type>image</type>
        <id>101</id>
        <posX>45</posX>
        <posY>1905</posY>
        <width>480</width>
        <height>480</height>
        <texture flipY="true" diffuse="Thumb_Mask.png">thumbs\ars_article.png</texture>
        <keepaspectratio>yes</keepaspectratio>
        <visible>control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <description>Channel name label</description>
        <type>fadelabel</type>
        <id>12</id>
        <posX>45</posX>
        <posY>120</posY>
        <label>-</label>
        <font>font22</font>
        <align>left</align>
        <width>570</width>
        <textcolor>90ffffff</textcolor>
        <visible>control.hasfocus(50)</visible>
        <animation effect="fade" time="250">visiblechange</animation>
      </control>
      <control>
        <type>group</type>
        <description>group element</description>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <onup>50</onup>
          <ondown>50</ondown>
          <textYOff>12</textYOff>
          <textXOff>-60</textXOff>
        </control>
      </control>
    </control>
    <control>
      <type>image</type>
      <id>0</id>
      <posX>-579</posX>
      <posY>4</posY>
      <width>627</width>
      <height>1071</height>
      <texture>bg_left.png</texture>
      <colorDiffuse>ffffffff</colorDiffuse>
      <visible>![control.hasfocus(2)|control.hasfocus(4)]</visible>
      <animation effect="fade" time="50">WindowClose</animation>
      <animation effect="fade" time="50">WindowOpen</animation>
      <animation effect="fade" time="250">visiblechange</animation>
    </control>
    <control>
      <type>actiongroup</type>
      <description>action menu</description>
      <defaultcontrol>2</defaultcontrol>
      <onexit>0</onexit>
      <buttonX>0</buttonX>
      <buttonY>0</buttonY>
      <buttonwidth>84</buttonwidth>
      <buttonheight>1080</buttonheight>
      <textureFocus>-</textureFocus>
      <textureNoFocus>-</textureNoFocus>
      <width>637</width>
      <height>1080</height>
      <dimColor>ffffffff</dimColor>
      <visible>[control.hasfocus(2)|control.hasfocus(4)]</visible>
      <animation effect="fade" time="250" start="75" reversible="false">visible</animation>
      <animation effect="slide" tween="circle" easing="out" time="250" start="-312,0" reversible="false">visible</animation>
      <animation effect="fade" time="250" end="75" reversible="false">hidden</animation>
      <animation effect="slide" tween="circle" easing="out" time="250" end="-312,0" reversible="false">hidden</animation>
      <animation effect="slide" time="150" end="-400,0" reversible="false">WindowClose</animation>
      <animation effect="slide" tween="circle" easing="out" time="250" start="-312,0" reversible="false">WindowOpen</animation>
      <animation effect="fade" time="250" start="75" reversible="false">WindowOpen</animation>
      <control>
        <type>image</type>
        <id>0</id>
        <posX>0</posX>
        <posY>0</posY>
        <width>627</width>
        <height>1080</height>
        <texture>bg_left.png</texture>
        <colorDiffuse>ffffffff</colorDiffuse>
      </control>
      <control>
        <id>0</id>
        <type>label</type>
        <posX>45</posX>
        <posY>52</posY>
        <label>#header.label</label><width>500</width>
        <font>font24</font>
        <align>left</align>
        <textcolor>ffffffff</textcolor>
      </control>
      <control>
        <type>image</type>
        <id>0</id>
        <posX>0</posX>
        <posY>71</posY>
        <width>579</width>
        <height>79</height>
        <texture>bg_button_nofocus.png</texture>
      </control>
      <control>
        <type>group</type>
        <layout>StackLayout(0, Vertical, true)</layout>
        <posX>0</posX>
        <posY>150</posY>
        <control>
          <description>RSS feed select</description>
          <type>button</type>
          <id>4</id>
          <label>706</label>
          <onleft>4</onleft>
          <onright>50</onright>
          <onup>17</onup>
          <ondown>2</ondown>
        </control>
        <control>
          <description>Refresh Button</description>
          <type>button</type>
          <id>2</id>
          <label>184</label>
          <onleft>2</onleft>
          <onright>50</onright>
          <onup>4</onup>
          <ondown>4</ondown>
        </control>
      </control>
    </control>
  </controls>
</window>