        <control>
          <description>Last searches</description>
          <type>button</type>
          <id>7</id>
          <label>Search history</label>
          <onup>2</onup>
          <ondown>6</ondown>
        </control>
        <control>
          <description>Search fields (Song, Album, Artist, etc.)</description>
          <type>button</type>
          <id>6</id>
          <label>Search fields</label>
          <ondown>3</ondown>
          <onup>7</onup>
        </control>
        <control>
