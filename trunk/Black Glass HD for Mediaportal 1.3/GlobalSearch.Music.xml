      </control>
      <control>
        <type>fadelabel</type>
        <description>Current program title</description>
        <id>1</id>
        <posX>45</posX>
        <posY>645</posY>
        <width>540</width>
        <font>font22</font>
        <label>#selecteditem</label>
        <wrapString> : : : </wrapString>
      </control>
