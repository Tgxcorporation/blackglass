          <id>7</id>
          <label>#GlobalSearch.Translation.SearchHistory.Label</label>
          <onright>50</onright>
          <onup>2</onup>
          <ondown>6</ondown>
        </control>
        <control>
          <type>button</type>
          <description>Search Fields</description>
          <id>6</id>
          <label>#GlobalSearch.Translation.SearchFields.Label</label>
